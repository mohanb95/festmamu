<% include ../partials/header %>
<%include ../partials/navbar%>
<link rel="stylesheet" type="text/css" href="/stylesheets/artists_detail.css">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<div class="container-fluid">
	
		<div id="myCarousel" class="carousel slide" data-ride="carousel">
		  <!-- Indicators -->
		  <% var link1=(headliner.image_link).split(","),i ; %>
		  <ol class="carousel-indicators">
		  	<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
		  	<% for(i=1;i<link1.length;i++){ %>
		    <li data-target="#myCarousel" data-slide-to="<%= i %>"></li>
		    <% } %>
		  </ol>

		  <!-- Wrapper for slides -->
		  <div class="carousel-inner" role="listbox" style=" width:100%; height: 50%;">
		    <div class="item active">
		      <img src="https://s3-us-west-2.amazonaws.com/festmamu/<%= link1[0] %>">
		    </div>
		    
		    <% for(i=1;i<link1.length;i++){ %>
		    <div class="item">
		      <img src="https://s3-us-west-2.amazonaws.com/festmamu/<%= link1[i] %>">
		    </div>
		    <% } %>
		    <div class="carousel-caption">
		    	<h2 class= "main-name"><%= headliner.StageName %></h2>
		    	</div>
		  </div>
		  

		  <!-- Left and right controls -->
		  <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
		    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
		    <span class="sr-only">Previous</span>
		  </a>
		  <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
		    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
		    <span class="sr-only">Next</span>
		  </a>
		</div>
	

	<div class="row" style="margin-top: 4px ">
		<div class="col-md-3 col-xs-3" style="border-right: 1px solid black;">
			<h2><%= headliner.Aname %></h2>
			<p>Price per hour: Rs.<%= headliner.price_per_hour %></p>
			<% var exp=2017-headliner.Year %>
			<p><%= exp %> years in the field</p>
		</div>
		<div class="col-md-6 col-md-offset-1 col-xs-6 col-xs-offset-1">
			<h4><%= headliner.Description %></h4>
		</div>
	</div>

<div class="container">
	<div class="row">
		<div class="col-md-12 col-xs-12">
		<h1>FAQ</h1>
		<ol>
			<li>what is some thing?</li>
			ans: some crap.
		</ol>
		</div>
	</div>
</div>
<script type="text/javascript" src="/js/rating.min.js"></script>
<%include ../partials/comments.ejs %>
<script type="text/javascript" src="/js/artist_comments.js"></script>


</div>

	<script   src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="   crossorigin="anonymous"></script>
	<!-- Latest compiled and minified JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.0/jquery.waypoints.js"></script>
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.0/shortcuts/inview.min.js"></script>
	
	<script src="https://cdn.jsdelivr.net/scrollreveal.js/3.1.4/scrollreveal.min.js"></script>
	<!-- <script type="text/javascript" src="js/star-rating.min.js"></script> -->
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/js/materialize.min.js"></script>
	<script type="text/javascript" src="/js/artist.js"></script>
</body>
</html>